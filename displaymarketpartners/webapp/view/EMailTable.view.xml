<mvc:View xmlns:core="sap.ui.core" 
		  xmlns="sap.m" 
		  xmlns:smartTable="sap.ui.comp.smarttable" 
		  xmlns:mvc="sap.ui.core.mvc"
		  xmlns:reuseTable="com.sap.cd.maco.mmt.ui.reuse.table"
		  xmlns:c="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
		  controllerName="com.sap.cd.maco.selfservice.ui.app.displaymarketpartners.view.EMailTable" 
		  height="100%">
	  <smartTable:SmartTable id="idEmailSmartTable"
						    entitySet="{this>/entitySet}"
						    tableType="ResponsiveTable" 
						    enableAutoBinding="false" 
						    beforeRebindTable="onBeforeRebindTable"
						    ignoredFields="" 
						    useExportToExcel="false" 
						    useTablePersonalisation="false" 
						    demandPopin="true" 
						    useVariantManagement="false"
						    showRowCount="true" 
						    placeToolbarInTable="true" 
						    header="{i18n>EMAIL_ADDRESS_SECTION_LBL}" >
	    <Table id="idEmailTable" 
		      growing="true" 
		      growingScrollToLoad="false" 
		      growingThreshold="10"
		      mode="None" >
	      <columns>
			 <Column>
              <Text text="{i18n>VALIDITY_COLUMN_LABEL}"/>
              <customData>
                <core:CustomData key="p13nData" 
                  value='\{"columnIndex":"1", "columnKey":"ValidFrom", "leadingProperty":["ValidFrom", "ValidTo"] }'/>
              </customData>
            </Column>
	      </columns>
	      <items>
	        <ColumnListItem>
	          <cells>
	            <VBox>
	            	<Text text =" { path: 'ValidFrom', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' } }" />
					<Text text ="{ path: 'ValidTo', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' } }"/>
	            </VBox>
	          </cells>
	        </ColumnListItem>
	      </items>
	    </Table>
	  </smartTable:SmartTable>
</mvc:View>