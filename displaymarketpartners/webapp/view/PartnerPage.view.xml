<mvc:View
	controllerName="com.sap.cd.maco.selfservice.ui.app.displaymarketpartners.view.PartnerPage"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:c="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:core="sap.ui.core"
	xmlns:uxap="sap.uxap"
	xmlns:button="com.sap.cd.maco.mmt.ui.reuse.button"
	xmlns:layout="sap.ui.layout">
	<uxap:ObjectPageLayout id="objectPage" upperCaseAnchorBar="false">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle primaryArea="Left">
				<uxap:expandedHeading>
					<Title text="{Name}" wrapping="true" class="sapUiTinyMarginTop"/>
				</uxap:expandedHeading>
				<uxap:snappedHeading>
					<Title text="{Name}" wrapping="true" class="sapUiTinyMarginTop"/>
				</uxap:snappedHeading>
				<uxap:expandedContent>
					<Text text="{Pid}" />
				</uxap:expandedContent>
				<uxap:snappedContent>
					<Text text="{Pid}"/>
				</uxap:snappedContent>
				<uxap:actions>
					<button:Update id="upload" c:action="update" press="onAction" type="Emphasized" />
					<button:Delete id="delete" c:action="delete" press="onAction" type="Transparent" />
				</uxap:actions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:headerContent>
			<layout:VerticalLayout class="sapUiSmallMarginEnd">
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/ValidFrom/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{
						path: 'ValidFrom',
						type: 'sap.ui.model.type.Date',
						formatOptions: { style: 'medium' }
					}"/>
				</HBox>
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/ValidTo/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{
						path: 'ValidTo',
						type: 'sap.ui.model.type.Date',
						formatOptions: { style: 'medium' }
					}"/>
				</HBox>
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/EmailContact/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{EmailContact}"/>
				</HBox>
			</layout:VerticalLayout>
			<layout:VerticalLayout class="sapUiSmallMarginEnd">
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/MarketRole/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{
						path: 'MarketRole',
						formatter: '.valueHelpFormatter.marketRole'
					}"/>
				</HBox>
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/Division/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{
						path: 'Division',
						formatter: '.valueHelpFormatter.division'
					}"/>
				</HBox>
				<HBox>
					<ObjectAttribute title="{/PartnerMasterData/MPIDqualifiers/#@sap:label}" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{MPIDqualifiers}"/>
				</HBox>
			</layout:VerticalLayout>
			<layout:VerticalLayout visible="{app>/devMode/enabled}">
				<HBox>
					<ObjectAttribute title="UUID" class="sapUiTinyMarginEnd"/>
					<ObjectAttribute text="{UUID}" />
				</HBox>
			</layout:VerticalLayout>
		</uxap:headerContent>
		<uxap:sections>
			<uxap:ObjectPageSection title="{i18n>emailTitlePlural}" titleUppercase="false">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
						<uxap:blocks>
							<mvc:XMLView id="emailTable" viewName="com.sap.cd.maco.mmt.ui.app.mgrpartners.page.partner.EMailTable"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="{i18n>as2TitlePlural}" titleUppercase="false">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
						<uxap:blocks>
							<mvc:XMLView id="as2Table" viewName="com.sap.cd.maco.mmt.ui.app.mgrpartners.page.partner.AS2Table"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="{i18n>certificateTitlePlural}" titleUppercase="false">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
						<uxap:blocks>
							<mvc:XMLView id="as2Table" viewName="com.sap.cd.maco.mmt.ui.app.mgrpartners.page.partner.AS2Table"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
</mvc:View>