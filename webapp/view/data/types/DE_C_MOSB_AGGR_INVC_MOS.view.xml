<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:form="sap.ui.layout.form"
	xmlns:smartTable="sap.ui.comp.smarttable" xmlns:smartfield="sap.ui.comp.smartfield"
	controllerName="com.sap.cd.maco.monitor.ui.app.displayprocesses.view.data.types.DataTable"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	
	<smartTable:SmartTable id="idTable"
					        entitySet="xMP4GxC_MOSB_AGGR_INVC_MOS"
					        tableType="ResponsiveTable"
					        demandPopin="true"
					        useExportToExcel="false"
					        useTablePersonalisation="false"
					        enableAutoBinding="true" 
					        requestAtLeastFields="path,ProcessDocumentKey,ProcessDocumentType"
					        beforeRebindTable="onBeforeRebindTable"
					        custom:entityName="xMP4GxC_MOSB_AGGR_INVC_MOS">
		
	  <smartTable:customToolbar>
	  	<Toolbar height="0rem"/>
	  </smartTable:customToolbar>
	  
	  <Table id="idProcessTypeTable"
	          growing="true"
	          growingScrollToLoad="true" 
	          growingThreshold="10" 
	          mode="None"
	          sticky="ColumnHeaders,HeaderToolbar" >
          <columns>
            <Column>
              <Text text="{/xMP4GxC_MOSB_AGGR_INVC_MOS/InvoiceDoc/#@sap:label}"/>
              <customData>
                <core:CustomData key="p13nData" 
                				 value='\{"columnIndex":"0", 
                						  "columnKey":"InvoiceDoc", 
                						  "leadingProperty":["InvoiceDoc"] }'/>
              </customData>
            </Column>
          </columns>
          <items>
            <ColumnListItem type="Inactive">
              <cells>
                  <smartfield:SmartField value="{InvoiceDoc}"/>
              </cells>
            </ColumnListItem>
          </items>
        </Table>
	</smartTable:SmartTable>
	
</mvc:View>