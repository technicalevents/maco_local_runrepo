<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="com.sap.cd.maco.monitor.ui.app.displayprocesses.view.data.ProcessGeneral"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:smartTable="sap.ui.comp.smarttable" xmlns:smartfield="sap.ui.comp.smartfield">
	
		<smartTable:SmartTable entitySet="xMP4GxC_General_Inbound_Info"
						        tableType="ResponsiveTable"
						        demandPopin="true"
						        useExportToExcel="false"
						        useTablePersonalisation="false"
						        enableAutoBinding="true" 
						        requestAtLeastFields="Path,ProcessDocumentKey,ProcessDocumentType"
						        beforeRebindTable="onBeforeRebindTable">
	 
	  <smartTable:customToolbar>
	  	<Toolbar height="0rem"/>
	  </smartTable:customToolbar>
	  
	  <Table growing="true"
	          growingScrollToLoad="true" 
	          growingThreshold="50" 
	          mode="None"
	          sticky="ColumnHeaders,HeaderToolbar" >
          <columns>
            <Column>
              <Text text="{/xMP4GxC_Cp_Adv_Recv/ContactName/#@sap:label}"/>
              <customData>
                <core:CustomData key="p13nData" 
                				 value='\{"columnIndex":"0", 
                						  "columnKey":"ContactName", 
                						  "leadingProperty":["ContactName"] }'/>
              </customData>
            </Column>
            <Column>
              <Text text="{/xMP4GxC_Cp_Adv_Recv/CommunicationAddress/#@sap:label}"/>
              <customData>
                <core:CustomData key="p13nData" 
                				 value='\{"columnIndex":"2", 
                						  "columnKey":"CommunicationAddress", 
                						  "leadingProperty":["CommunicationAddress"] }'/>
              </customData>
            </Column>
            <Column>
              <Text text="{/xMP4GxC_Cp_Adv_Recv/CommQualifier/#@sap:label}"/>
              <customData>
                <core:CustomData key="p13nData" 
                				 value='\{"columnIndex":"3", 
                						  "columnKey":"CommQualifier", 
                						  "leadingProperty":["CommQualifier"] }'/>
              </customData>
            </Column>
          
            
          </columns>
          <items>
            <ColumnListItem type="Inactive">
              <cells>
                  <smartfield:SmartField value="{ContactName}"/>
                  <smartfield:SmartField value="{CommunicationAddress}"/>
                  <smartfield:SmartField value="{CommQualifier}"/>
              </cells>
            </ColumnListItem>
          </items>
        </Table>
	</smartTable:SmartTable>
</mvc:View>