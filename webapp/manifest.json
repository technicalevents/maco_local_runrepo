{
  "_version": "1.4.0",
  "sap.app": {
    "id": "com.sap.cd.maco.monitor.ui.app.displayprocesses",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{APP_TITLE}}",
    "description": "{{APP_TITLE}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "MACO-MMT",
    "dataSources": {
      "maindatasource": {
        "uri": "/sap/opu/odata/MP4G/PROCESSHEADER_SB",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml",
          "annotations": ["_MP4G_PROC_ANNO_MDL"]
        }
      },
      
      "_MP4G_PROC_ANNO_MDL": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='%2FMP4G%2FPROCESSHEADER_SB_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/_MP4G_PROC_ANNO_MDL.xml"
        }
      },

      "localAnnotations": {
        "uri": "annotation/Process.xml",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "annotation/Process.xml"
        }
      }
    },
    "resources": "resources.json",
    "crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "Process",
					"action": "display",
					"title": "{{APP_TITLE}}",
					"icon": "sap-icon://process"
				}
			}
		}
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": true,
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": false,
      "phone": false
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.sap.cd.maco.mmt.ui.reuse.root.Root",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.56.1",
      "libs": {
        "sap.m": {
        },
        "sap.f": {
        },
        "sap.ui.core": {
        },
        "sap.ui.layout": {
        },
        "sap.uxap": {
        },
        "sap.ui.comp": {
        },
        "sap.ui.table": {
        },
        "com.sap.cd.maco.mmt.ui.reuse":{
        	"minVersion": "2.0.0"
        },
        "sap.suite.ui.commons":{
        }

      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "resources": {
      "css": [
        { "uri": "css/style.css" }
      ]
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.cd.maco.monitor.ui.app.displayprocesses.i18n.i18n"
        }
      },
      "": {
        "dataSource": "maindatasource",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "useBatch": true
        }
      }
    },
    "services": {
      "ShellUIService": {
        "factoryName": "sap.ushell.ui5service.ShellUIService",
        "lazy": false,
        "settings": {
          "setHierarchy": "auto",
          "setTitle": "auto"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "name": "initial",
          "pattern": "",
          "target": [
            "listReport"
          ]
        },
        {
          "name": "listReport",
          "pattern": "listReport",
          "target": [
            "listReport"
          ]
        },
        {
          "name": "localstate-new",
          "pattern": "{?query}",
          "target": [
            "listReport"
          ]
        },
        {
          "name": "processPage",
          "pattern": "processPage/{ProcessDocumentKey},{ProcessID}",
          "target": [
            "processPage"
          ]
        }
      ],
      "targets": {
        "listReport": {
          "viewPath": "com.sap.cd.maco.monitor.ui.app.displayprocesses.view",
          "viewName": "ProcessListReport",
          "viewId": "listReport",
          "viewLevel": 1,
          "title": "{i18n>APP_TITLE}"
        },
        "processPage": {
          "viewPath": "com.sap.cd.maco.monitor.ui.app.displayprocesses.view",
          "viewName": "ProcessPage",
          "viewId": "processPage",
          "viewLevel": 2,
          "title": "{i18n>PROCESS_SINGULAR_LBL}"
        },
        "notFound": {
          "viewPath": "com.sap.cd.maco.mmt.ui.reuse.nav",
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    }
  }
}