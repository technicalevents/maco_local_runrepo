{
  "_version": "1.4.0",
  "sap.app": {
    "id": "com.sap.cd.maco.monitor.ui.app.definecontactrules",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{APP_TITLE}}",
    "description": "{{APP_TITLE}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "MACO-MMT",
    "dataSources": {
      "Main": {
        "uri": "/sap/opu/odata/MP4G/??/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml",
          "annotations": [
            "Message"
          ]
        }
      },
      "Message": {
        "type": "ODataAnnotation",
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='%2FMP4G%2F??_VAN',Version='0001')/$value/",
        "settings": {
          "localUri": "annotation/Message.xml"
        }
      }
    },
    "resources": "resources.json",
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "UtilsDataExchangeProcessing",
          "action": "displayMessage",
          "title": "{{APP_TITLE}}",
          "icon": "sap-icon://email"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": true,
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": false,
      "phone": false
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.sap.cd.maco.mmt.ui.reuse.controller.app.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.56.1",
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.uxap": {},
        "sap.ui.comp": {},
        "sap.ui.table": {},
        "com.sap.cd.maco.mmt.ui.reuse": {
          "minVersion": "2.0.0"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.cd.maco.monitor.ui.app.definecontactrules.i18n.i18n"
        }
      },
      "": {
        "dataSource": "Main",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "useBatch": true
        }
      }
    },
    "services": {
      "ShellUIService": {
        "factoryName": "sap.ushell.ui5service.ShellUIService",
        "lazy": false,
        "settings": {
          "setTitle": "auto"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "name": "initial",
          "pattern": "",
          "target": [
            "listReport"
          ]
        },
        {
          "name": "listReport",
          "pattern": "listReport",
          "target": [
            "listReport"
          ]
        },
        {
          "name": "localstate-new",
          "pattern": "{?query}",
          "target": [
            "listReport"
          ]
        }
      ],
      "targets": {
        "listReport": {
          "viewPath": "com.sap.cd.maco.monitor.ui.app.definecontactrules.view",
          "viewName": "ContactRulesListReport",
          "viewId": "listReport",
          "viewLevel": 1,
          "title": "{i18n>APP_TITLE}"
        },
        "notFound": {
          "viewPath": "com.sap.cd.maco.mmt.ui.reuse.controller.notFound",
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    }
  },
  "sap.platform.cf": {
    "oAuthScopes": [
      "$XSAPPNAME.AppMktMsg_Monitor"
    ]
  }
}