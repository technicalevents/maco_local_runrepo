<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ActionSheet placement="Bottom">
			<Button text="{i18n>INVOICE_BTN_TXT}" 
					press="onExecuteMsgAggrItemSelected('Invoice', $event)" />
			<Button text="{i18n>ENERGY_VALUES_BTN_TXT}" 
					press="onExecuteMsgAggrItemSelected('Energy Values', $event)" />
			<Button text="{i18n>METER_READING_RESULTS_BTN_TXT}"  
					press="onExecuteMsgAggrItemSelected('Meter Reading Results', $event)" />
			<Button text="{i18n>TIME_SERIES_DATA_BTN_TXT}" 
					press="onExecuteMsgAggrItemSelected('Time Series Data', $event)" />
	</ActionSheet>
</core:FragmentDefinition>