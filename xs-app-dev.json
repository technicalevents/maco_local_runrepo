{
  "welcomeFile": "/webapp/flp/FLPSandbox.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/resources/com/sap/cd/maco/mmt/ui/reuse/(.*)$",
      "target": "/src/com/sap/cd/maco/mmt/ui/reuse/$1",
      "localDir": "node_modules/com-sap-cd-maco-mmt-ui-reuse"
    },
    {
      "source": "^/resources",
      "target": "/resources",
      "destination": "sapui5"
    },
    {
      "source": "^/test-resources",
      "target": "/test-resources",
      "destination": "sapui5"
    },
    {
      "source": "^/webapp/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "^/odata/v2/masterdataservice/(.*)$",
      "target": "$1",
      "destination": "backendservice"
    }
  ]
}